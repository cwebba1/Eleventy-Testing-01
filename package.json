    "lightningcss": "^1.24.1",
    "luxon": "^3.5.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.0.1",
    "nunjucks": "^3.2.4",
    "outdent": "^0.8.0",
    "postcss-custom-selectors": "^8.0.1"
  },
  "devDependencies": {
